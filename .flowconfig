[ignore]
.*/node_modules/radium
<PROJECT_ROOT>/dist
<PROJECT_ROOT>/node_modules/immutable
<PROJECT_ROOT>/node_modules/stylelint
<PROJECT_ROOT>/node_modules/flow-coverage-report

[include]

[libs]
flow-typed

[lints]
all=warn

[options]
include_warnings=true
esproposal.class_static_fields=enable
module.ignore_non_literal_requires=true
module.file_ext=.js
module.file_ext=.css
module.name_mapper='^\(.*\)\.css$' -> '<PROJECT_ROOT>/interfaces/CSSModule.js'
