{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "> 1%"],
        "node": "current"
      },
      "debug": true
    }],
    "react"
  ],
  "plugins": [
    "transform-object-rest-spread",
  ],
  "env": {
    "RELEASE": {
      "plugins": [
        ["css-modules-transform", {
          "generateScopedName": "[name]-[local]_[hash:base64:5]",
          "extractCss": "./dist/stylesheets/local.css",
          "append": [
            "./src/theme",
            "autoprefixer"
          ]
        }]
      ]
    }
  }
}
